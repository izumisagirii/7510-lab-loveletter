<MySwiper@MDSwiperItem>
    MDCard:
        radius: dp(20)
        md_bg_color: app.theme['button']
        size_hint_x: 1
        MDAnchorLayout:
            MDBoxLayout:
                orientation: "vertical"
                FitImage:
                    id:image
                    source: "group_assets/images/image.png"
                    size_hint_y: .5
                    pos_hint: {"top": 1 , "center_x": .5}
                    radius: [dp(20),dp(20),dp(20),dp(0)]
                MDLabel:
                    id:text
                    text: "textLabel"
                    theme_text_color: "Custom"
                    text_color: app.theme['text']
                    size_hint_x: .8
                    size_hint_y: .5
                    pos_hint: {"top": .5 , "center_x": .5}
                    font_style: "H5"
            MDFloatLayout:
                MDFillRoundFlatIconButton:
                    id: heart_button
                    icon: "cards-heart-outline"
                    text:'0'
                    icon_size: dp(30)
                    font_size: dp(25)
                    icon_color:'000000'
                    md_bg_color: app.theme['button']
                    pos_hint: {"center_x": .75, "center_y": .5}
                    on_release:root.heart()
                FitImage:
                    id:profile
                    radius: dp(40)
                    source: "group_assets/images/l_hires.jpg"
                    size_hint: None, None
                    size:  dp(80),dp(80)
                    pos: root.pos[0], root.pos[1]+root.height/2-dp(35)
                MDLabel:
                    id:textName
                    text: "nameLabel"
                    theme_text_color: "Custom"
                    text_color: app.theme['text']
                    pos: root.pos[0]+dp(80), root.pos[1]-dp(20)
                    font_size: dp(18)

            


<MySwiperHD@MDSwiperItem>
    MDCard:
        radius: dp(20)
        md_bg_color: app.theme['button']
        size_hint_x: 1
        MDAnchorLayout:
            MDBoxLayout:
                orientation: "horizontal"
                FitImage:
                    id:image
                    pos_hint:{"top": .8}
                    source: "group_assets/images/image.png"
                    radius: [dp(20),0,0,dp(20)]
                    size_hint_x: .5
                    pos_hint: {"left": 1}
                MDFloatLayout:
                    size_hint_x: .5
                    MDLabel:
                        id:text
                        text: "MDLabel"
                        theme_text_color: "Custom"
                        text_color: app.theme['text']
                        font_style: "H5"
                        size_hint_x: .8
                        pos_hint: {"right": .9}
            MDFloatLayout:
                MDFillRoundFlatIconButton:
                    id: heart_button
                    icon: "cards-heart-outline"
                    text:'0'
                    icon_size: dp(30)
                    font_size: dp(25)
                    icon_color:'000000'
                    md_bg_color: app.theme['button']
                    pos_hint: {"center_x": .13, "center_y": .1}
                    on_release:root.heart()
                FitImage:
                    id:profile
                    radius: dp(40)
                    source: "group_assets/images/image.png"
                    size_hint: None, None
                    size:  dp(80),dp(80)
                    pos: root.pos[0]+root.width/2-dp(40), root.pos[1]+root.height-dp(80)
                MDLabel:
                    id:textName
                    text: "nameLabel"
                    theme_text_color: "Custom"
                    text_color: app.theme['text']
                    pos: root.pos[0]+root.width/2+dp(60), root.pos[1]+root.height/2-dp(30)
                    font_size: dp(20)

                
                
                

<ScreenBrowse>:
    MDLabel:
        size_hint_y: 1
        pos_hint: {"bottom": 1}
        md_bg_color: app.theme['bg']
        

    MDSwiper:
        id: swiper
        size_hint_y: None
        height: root.height - dp(70)
        y: root.height - self.height - dp(50)
        on_swipe_right:root.swipe()


    MDFloatingActionButton:
        on_release: 
            #root.swipe_right()
            app.back_to_main()
        icon:'keyboard-backspace'
        icon_color: app.theme['icon']
        md_bg_color: app.theme['button']
        pos: dp(10), root.height-self.height-dp(10)